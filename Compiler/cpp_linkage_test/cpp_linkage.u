fn U32ToStr( u32 mut x, [ i8, 64 ] &mut out_str )
{
	auto constexpr zero_char= 48u;
	if( x == 0u )
	{
		out_str[0u]= i8(zero_char);
		out_str[1u]= 0i8;
		return;
	}
	var u32 mut dst= 0u;
	var u32 mut div= 1000000000u;

	while( div > x )
	{
		div/= 10u;
	}

	while( div > 0u )
	{
		auto imut digit= x / div;
		out_str[dst]= i8( digit + zero_char );
		++dst;
		x-= digit * div;
		div/= 10u;
	}
	out_str[dst]= 0i8;
}


//##success_test
import "../ustlib/mixin_utils.u"

fn nomangle main() : i32
{
	{
		var ust::mixin_utils::static_string</32s/> s{};
		static_assert( s == "" );
	}
	{
		var ust::mixin_utils::static_string</32s/> mut s;
		s+= "foo";
		s+= "66";
		halt if( s != "foo66" );
		halt if( s == "foo" );
		halt if( s == "66" );
		halt if( "foo66" != s );
		halt if( "foo" == s );
		halt if( "66" == s );
	}
	return 0;
}

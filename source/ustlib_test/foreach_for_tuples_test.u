fn U_Main() : i32
{
	{
		var tup[ i32, u32, f32 ] t[ 4, 8u, 15.0f ];
		var i32 mut sum= 0;
		foreach( x : t ) { sum+= i32(x); }
		halt if( sum != 4 + 8 + 15 );
	}
	{
		var tup[ u32, f32, i32 ] mut t[ 54u, -65.0f, 895 ];
		var size_type mut iterations(0);
		foreach( &mut x : t )
		{
			x*= typeof(x)(-2);
			++iterations;
		}
		halt if( iterations != size_type(3) );
		halt if( t[0u] != -108u );
		halt if( t[1u] != 130.0f );
		halt if( t[2u] != -1790 );
	}
	{
		var tup[] t[];
		var size_type mut iterations(0);
		foreach( x : t ) { ++iterations; }
		halt if( iterations != size_type(0) );
	}

	return 0;
}

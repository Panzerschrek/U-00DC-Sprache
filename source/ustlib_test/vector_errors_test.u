	}
	{ // "push_back" does reference-pollution.
		var i32 mut x= 0;
		var ust::vector</RI/> mut r;

		r.push_back( RI(x) );
		++x; //  UNexpect_error ReferenceProtectionError
	}
	{ // "front" returns correct inner reference.

import "../../imports/stdout.u"
import "unix.uh"

namespace ust
{

fn stdout_print( array_view_imut</char8/> str )
{
	unsafe( ::write( STDOUT_FILENO, ust::ptr_cast_to_byte8(str.data()), str.size() ) );
	// TODO - halt if write fails
}

fn stderr_print( array_view_imut</char8/> str )
{
	unsafe( ::write( STDERR_FILENO, ust::ptr_cast_to_byte8(str.data()), str.size() ) );
	// TODO - halt if write fails
}

} // namespace ust
